<section>
  <div class="weather-side m1">
    {{>weather}}
  </div>

  {{> todos}}

  <div class="parent-side right-side">
    <div class="parent links flex-col">
      <a class="link-title link spreadButton"><i class="fas fa-link"></i> My Links</a>
    </div>
    <div class="parent folders flex-col">
      <div class="folder-title folder spreadButton"><i class="far fa-folder-open"></i> My Folder</div>
    </div>
  </div>

  <div class="buttons">
    <form class="deleteFolder dp-none">
      <button type="submit"></button>
    </form>
    <form action="/login/logout" method="GET" class="logout">
      <button type="submit" class="logout-btn">Logout</button>
    </form>
  </div>

</section>

<script type="module" src="./js/displayFolder.js"></script>